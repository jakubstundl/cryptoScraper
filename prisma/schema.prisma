generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model coinCodexPrediction {
  name   String  @id @unique
  time   Float?
  days      String? @db.Text
  values      String? @db.Text
  percentages      String? @db.Text
}


model coinTradeData {
  name   String  @id @unique
  count Float?
  boughtFor Float?
  boughtAt Float?
  sellAt Float?
  
}